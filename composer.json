{
    "name": "KolossusD/CyxAuthBundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Cyx custom login bundle with Rest API",
    "autoload": {
        "psr-4": { "KolossusD\\CyxAuthBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "require": {
        "php": ">=5.5.9",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "captcha-com/symfony-captcha-bundle": "4.*",
	
        "nelmio/cors-bundle": "^1.4",
        "nelmio/api-doc-bundle": "^2.13",
        "friendsofsymfony/rest-bundle": "^2.0@dev",
        "jms/serializer-bundle": "^1.1",
        "willdurand/faker-bundle": "^1.3",
        "knplabs/knp-paginator-bundle": "^2.5",
        "lexik/jwt-authentication-bundle": "^1.7"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.0",
        "behat/behat": "^3.1",
        "behat/symfony2-extension": "^2.1",
        "guzzlehttp/guzzle": "^6.2",
        "csa/guzzle-bundle": "^2.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "dev-master"
        }
    }
}
